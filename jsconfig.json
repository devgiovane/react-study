{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~@Adapters/*": [
                "./src/adapters/*"
            ],
            "~@Assets/*": [
                "./src/assets/*"
            ],
            "~@Components/*": [
                "./src/components/*"
            ],
            "~@Contexts/*": [
                "./src/contexts/*"
            ],
            "~@Pages/*": [
                "./src/pages/*"
            ],
            "~@Services/*": [
                "./src/services/*"
            ],
            "~@Template/*": [
                "./src/template/*"
            ]
        }
    }
}
